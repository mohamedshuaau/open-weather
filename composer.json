{
    "name": "mohamedshuaau/open-weather",
    "description": "A simple Open Weather API Manager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Shuaau",
            "email": "mohammedshuaau@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Shuaau\\OpenWeather\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shuaau\\OpenWeather\\WeatherServiceProvider"
            ],
            "aliases": {
                "Weather": "Shuaau\\OpenWeather\\Facades\\Weather"
            }
        }
    }
}
